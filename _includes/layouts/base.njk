<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ '/assets/img/apple-touch-icon.png' | url}}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ '/assets/img/favicon-32x32.png' | url}}"> 
    <link rel="icon" type="image/png" sizes="16x16" href="{{ '/assets/img/favicon-16x16.png' | url}}">  
    <title>{{ title }} - {{ site.title }}</title>
    <script src="{{ '/assets/uswds/js/uswds-init.min.js' | url }}"></script>
    <link rel="stylesheet" href="{{ '/assets/uswds/css/styles.css' | url }}">
  </head>
  <body>
    <a class="usa-skipnav" href="#main-content">Skip to main content</a>

    {% if site.banner.enabled %}
      {% include 'components/banner.njk' if site.banner.enabled %}
    {% endif %}

    {% if site.header.enabled %}
      {% include ['components/header.', site.header.type, '.njk'] | join %}
    {% endif %}

    {% if breadcrumb %}
      {% include 'components/breadcrumb.njk' %}
    {% endif %}

    {% if hero %}
      {% include 'components/hero.njk' %}
    {% endif %}

    {{ content | safe }}

    {% if site.footer.enabled %}
      {% include ['components/footer.', site.footer.type, '.njk'] | join %}
    {% endif %}

    {% if site.identifier.enabled %}
      {% include 'components/identifier.njk' %}
    {% endif %}

    <script src="{{ '/assets/uswds/js/uswds.min.js' | url }}"></script>

    <!-- We participate in the US government's analytics program. See the data at analytics.usa.gov. -->
    <script async type="text/javascript" src="https://dap.digitalgov.gov/Universal-Federated-Analytics-Min.js?agency=GSA" id="_fed_an_ua_tag"></script>

    <script src="{{ '/assets/js/fslightbox.js' | url }}"></script>
  </body>
</html>
